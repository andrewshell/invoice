{
    "name": "andrewshell/invoice",
    "type": "project",
    "description": "Simple Invoice Generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Shell",
            "email": "andrew@andrewshell.org"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "radar/adr": "~1.0",
        "relay/middleware": "~1.0",
        "zendframework/zend-diactoros": "~1.0",
        "symfony/yaml": "^3.0",
        "twig/twig": "^1.23",
        "puli/cli": "1.0.0-beta10",
        "puli/composer-plugin": "1.0.0-beta9",
        "puli/repository": "1.0.0-beta9",
        "puli/twig-extension": "1.0.0-beta8",
        "webmozart/key-value-store": "1.0.0-beta7"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1.*",
        "squizlabs/php_codesniffer": "2.5.*"
    },
    "autoload": {
        "psr-4": {
            "Invoice\\": "src/Invoice/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Invoice\\": "tests/Invoice/",
            "Aura\\Di\\": "vendor/aura/di/tests/",
            "Puli\\Repository\\Tests\\": "vendor/puli/repository/tests/"
        }
    },
    "minimum-stability": "dev"
}
